{
  "name": "@verndale/toolkit",
  "description": "Front end build system.",
  "license": "MIT",
  "version": "6.0.0",
  "author": {
    "name": "Verndale",
    "email": "hello@verndale.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/verndale/toolkit.git"
  },
  "scripts": {
    "check-deps": "check-dependencies --install",
    "js-lint": "eslint --fix --color ./src/js/**/*.*",
    "style-lint": "stylelint --fix --color ./src/scss/**/*.*",
    "prestart": "yarn check-deps",
    "start": "start-storybook -p 6006",
    "prebuild": "yarn check-deps && yarn js-lint && yarn style-lint",
    "build": "rimraf dist && webpack --env production --progress",
    "build-storybook": "build-storybook -o ./tools/component-library/ --no-dll",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "create-page": "node ./.scaffold/index page",
    "create-module": "node ./.scaffold/index module",
    "create-js": "node ./.scaffold/index js",
    "create-react": "node ./.scaffold/index react",
    "mock": "nodemon server/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@verndale/core": "1.1.3",
    "@verndale/front-end-components": "1.10.3",
    "check-dependencies": "1.1.0",
    "focus-visible": "5.2.0",
    "sass-mq": "6.0.0-beta.1",
    "webpack-svgstore": "2.0.2"
  },
  "devDependencies": {
    "@namics/stylelint-bem": "6.3.4",
    "@storybook/addon-a11y": "6.4.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/html": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "autoprefixer": "10.3.4",
    "chalk": "4.1.2",
    "chokidar": "3.5.3",
    "copy-webpack-plugin": "9.0.1",
    "core-js": "3.17.2",
    "cors": "2.8.5",
    "css-loader": "6.2.0",
    "esbuild-loader": "2.18.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "express": "4.17.1",
    "filewatcher-webpack-plugin": "1.2.0",
    "fs-extra": "10.0.0",
    "handlebars-loader": "1.7.1",
    "husky": "^7.0.0",
    "image-minimizer-webpack-plugin": "2.2.0",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-svgo": "9.0.0",
    "lint-staged": "11.1.2",
    "mini-css-extract-plugin": "2.2.2",
    "nanoid": "3.1.28",
    "nodemon": "2.0.12",
    "postcss": "8.4.5",
    "postcss-loader": "6.1.1",
    "postcss-scss": "4.0.2",
    "prettier": "2.4.1",
    "prompt-sync": "4.2.0",
    "regenerator-runtime": "0.13.9",
    "replace-in-file": "6.2.0",
    "rimraf": "3.0.2",
    "sass": "1.39.0",
    "sass-loader": "12.1.0",
    "style-loader": "3.3.1",
    "stylelint": "14.1.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-use-logical-spec": "3.2.2",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.1.6",
    "valid-filename": "3.1.0",
    "webpack": "5.52.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.8.0",
    "webpack-import-glob-loader": "1.6.3"
  }
}
