{
  "name": "@verndale/toolkit",
  "description": "Front end build system.",
  "license": "MIT",
  "version": "3.0.0",
  "author": {
    "name": "Verndale",
    "email": "hello@verndale.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/verndale/toolkit.git"
  },
  "scripts": {
    "version": "git add .",
    "postversion": "git push origin master --tags",
    "check-deps": "check-dependencies --install",
    "create-server": "cross-env NODE_ENV=production gulp create-server",
    "js-lint": "eslint --fix --color --debug --ignore-path .eslintignore ./src/js/**/*.*",
    "start": "yarn check-deps && cross-env NODE_ENV=development gulp",
    "build": "yarn js-lint && cross-env NODE_ENV=production gulp && webpack -p --env.production --progress && yarn create-server"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "normalize.css": "8.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-function-bind": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "browser-sync": "2.26.7",
    "check-dependencies": "1.1.0",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "del": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-detailed-reporter": "0.8.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-import": "2.20.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-clean-css": "4.3.0",
    "gulp-data": "1.3.1",
    "gulp-eslint": "6.0.0",
    "gulp-file-include": "2.1.1",
    "gulp-filelog": "0.4.1",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "7.1.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sass-glob": "1.1.0",
    "gulp-sass-lint": "1.4.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgstore": "7.0.1",
    "gulp-template": "5.0.0",
    "gulp-util": "3.0.8",
    "gulp-watch": "5.0.1",
    "html-loader": "1.1.0",
    "lodash": "4.17.15",
    "merge-stream": "2.0.0",
    "minimist": "1.2.5",
    "node-notifier": "7.0.0",
    "proxy-middleware": "0.15.0",
    "regenerator-runtime": "0.13.5",
    "uglify-js": "3.9.1",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.10.3"
  }
}
